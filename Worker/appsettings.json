{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Bounan": "Trace",
      "System": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "TimestampFormat": "HH:mm:ss.fff ",
        "IncludeScopes": true
      }
    },
    "AwsLogger": {
      "IncludeScopes": true,
      "DisableLogGroupCreation": true,
      "LogGroup": ""
    }
  },
  "SqsConfig": {
    "QueueUrl": "",
    "Threads": 1,
    "MessageTimeoutSeconds": 100
  },
  "VideoService": {
    "ConcurrentDownloads": 30,
    "TempVideoFilePattern": "/tmp/bounan-downloader/temp-video-{0}.mp4"
  },
  "Telegram": {
    "ApiUrl": "",
    "BotToken": "",
    "DestinationChatId": ""
  }
}
