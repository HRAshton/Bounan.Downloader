{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Bounan": "Debug",
      "System": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning",
      "Microsoft.Extensions.Http.DefaultHttpClientFactory": "Warning",
      "System.Net.Http": "Warning"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "TimestampFormat": "HH:mm:ss.fff ",
        "IncludeScopes": true
      }
    },
    "IncludeScopes": true,
    "DisableLogGroupCreation": true
  },
  "Processing": {
    "Threads": 2
  },
  "VideoService": {
    "ConcurrentDownloads": 30,
    "TempVideoFilePattern": "/tmp/bounan-downloader/temp-video-{0}.mp4",
    "TimeoutSeconds": 300
  }
}
